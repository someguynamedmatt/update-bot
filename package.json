{
  "name": "update-bot",
  "version": "1.0.0",
  "description": "A friendly little GitHub bot that will keep your PRs up-to-date with the head branch",
  "main": "bin/index.js",
  "bin": {
    "update-bot": "bin/index.js"
  },
  "scripts": {
    "grammar": "write-good *.md --no-passive",
    "markdownlint": "markdownlint *.md",
    "test": "npm run grammar && npm run markdownlint",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someguynamedmatt/update-bot.git"
  },
  "keywords": [
    "Node",
    "Bot"
  ],
  "author": "Matt <dev@mttyng.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/someguynamedmatt/update-bot/issues"
  },
  "homepage": "https://github.com/someguynamedmatt/update-bot#readme",
  "devDependencies": {
    "husky": "1.0.1",
    "markdownlint-cli": "0.13.0",
    "write-good": "0.13.1",
    "eslint": "5.0.1",
    "eslint-config-dollarshaveclub": "3.0.1"
  }
}
